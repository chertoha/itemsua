{{#each this}}
<li class="catalog__item">
    <section class="catalog__section" data-section-id="{{sectionId}}">
        <h2 class="catalog__label">{{sectionTitle}}</h2>
        <ul class="catalog-product-list">
            {{#each models}}
            <li class="catalog-product-list__item">
                <a class="catalog-card link" href="#" data-model-id="{{modelId}}">
                    <div class="catalog-card__thumb">
                        <img class="catalog-card__image" src="{{modelImageUrl}}" alt="{{modelTitle}}" width="100"
                            height="100" />
                    </div>
                    <div class="catalog-card__meta">
                        <h3 class="catalog-card__title">{{modelTitle}}</h3>
                        <p class="catalog-card__text">
                            {{modelText}}
                        </p>
                    </div>
                </a>
            </li>
            {{/each }}
        </ul>
    </section>
</li>
{{/each }}